<view class='page'>
  <view class='page__cd'>
    <view class='page__hd vip-head border_botom' wx:if='{{!isVip}}'>
      当前尚未开通
      <image src='data:image/png;base64,'></image>
    </view>
    <view class='page__hd vip-head border_botom' wx:if='{{isVip}}'>
      VIP到期时间{{dataTime}}
      <image src='data:image/png;base64,'></image>
    </view>
    <!--vip 特权  -->
    <view class='privilege list border_botom'>
      <view>
        <image src='data:image/png;base64,'></image>
        VIP特权</view>
      <view class='dynamics pri-logo'>
        <view>
          <view class='v-v'>
            <image src='data:image/png;base64,'></image>
          </view>
          <view>独特标识</view>
        </view>
        <view>
          <view class='v-v'>
            <image src='data:image/png;base64,'></image>
          </view>
          <view>VIP曲库</view>
        </view>
        <view>
          <view class='v-v'>
            <image src='data:image/png;base64,'></image>
          </view>
          <view>无损音乐</view>
        </view>
        <view>
          <view class='v-v'>
            <image src='data:image/png;base64,'></image>
          </view>
          <view>付费折扣</view>
        </view>
      </view>
    </view>
    <!--VIP购买  -->
    <view class='buys list border_botom padding-bottom-none'>
      <view>
        <image src='data:image/png;base64,'></image>
        VIP购买</view>
      <view class='list' wx:for="{{lists}}" wx:key='*this' id="{{item.id}}">
        {{item.alias}}
        <label class='red'>{{item.cost}}元</label>
        <label class='underline'>{{item.orig}}元</label>
        <button size='mini' class='miniBtn' wx:if='{{!isVip}}' data-id="{{item.id}}" bindtap='immeVip' data-mouth='1' data-money='{{item.cost}}'>开通</button>
        <button size='mini' class='miniBtn' wx:if='{{isVip}}'  data-id="{{item.id}}" bindtap='immeVip' data-mouth='1' data-money='{{item.cost}}'>续费</button>
      </view>
      <!--手动输入月  -->
      <view class='list border-none' bindtap='opening'>
        手动输入月数
        <image class='_more' src='../../images/right.png'></image>
      </view>
    </view>
    <!--激活码  -->
    <view class='list border_botom activeCode' bindtap='gotocode'>
      <image src='data:image/png;base64,'></image>
      激活码兑换
      <image class='_more' src='../../images/right.png'></image>
    </view>
  </view>
  <!--弹窗  -->
  <view class='tipsDark' wx:if="{{isMouth}}">
    <view class='red'>请输入开通月数</view>
    <view>
      <text>月数：</text>
      <label class='label' bindtap='numredu'>-</label>
      <input value="{{inputVal}}" />
      <label class='label' bindtap='numadd'>+</label>
    </view>
    <view>金额：<label class='red'>{{headmouth}}元</label>
      <label class='underline'>{{headlastmouth}}元</label>
    </view>
    <view  id='_immediate' data-mouth='{{headmouth}}' bindtap='immeVip' data-id='1' data-mouth='{{inputVal}}'>立即开通</view>
  </view>
  <!-- 遮罩 -->
  <view class='page__dark' bindtap='clearDark' wx:if="{{isMouth}}"></view>
  <!--背景  -->
  <view class='page__bg'></view>
</view>
