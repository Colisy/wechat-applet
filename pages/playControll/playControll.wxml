<view class='page'>
  <!--头部  -->
  <view class='page__hd playHeader'>
    <image src='{{obj.cover_url}}'></image>
    <text class='singName'>{{obj.title}}</text>
    <text class='singer'>{{obj.sing}}</text>
    <image wx:if='{{!isPlay}}' bindtap='controlPlay' class='playContrl' src='data:image/png;base64,'></image>
    <image wx:if='{{isPlay}}' bindtap='controlPause' class='pauseContrl' src='data:image/png;base64,'></image>
  </view>
  <!--歌词  -->
  <view class='page_content'>
    <view class='page_content_inner'>
      <view class='block'>{{obj.title}}-{{obj.sing}}</view>
      <view class='block'>词：{{obj.lyrics}}</view>
      <view class='block'>曲：{{obj.composer}}</view>
      <view class='block' wx:if='{{textShow}}'>{{objText}}</view>
      <view class='block' wx:if='{{!textShow}}' wx:for='{{objText}}' wx:key="*this">{{item}}</view>
    </view>
  </view>
  <!--collect、  give-like-->
  <view id='coomponents'>
    <view id='{{obj.id}}' bindtap='collect'>
      <image class='collect' src='data:image/png;base64,' wx:if='{{isCollect}}'></image>
      <image class='collect' wx:if='{{!isCollect}}' src='data:image/png;base64,'></image>
    </view>
    <image class='sup' src='data:image/png;base64,'></image>

  </view>
  <!--播放控件  -->
  <view id='playingControls'>
    <!--audio  -->
    <audio id="player" loop poster="{{player.poster}}" name="{{player.name}}" author="{{player.author}}" src="{{player.src}}" bindtimeupdate='timeupdate'></audio>
    <!--  -->
    <view class='float startTime'>{{startTime}}</view>
    <slider backgroundColor='#808080' class='float slider' min="0" max="{{totalPosition}}" value="{{currentPosition}}" activeColor='#ff5045'></slider>
    <view class='float endTime'>{{endTime}}</view>
    <!--防止点击  -->
    <view class='control_hd'></view>
  </view>
  <!--download  -->
  <button data-id="{{obj.id}}" data-src='{{obj.audio_url}}' class="weui-btn" id='download' type="primary" plain="true" bindtap='download'>
    <image src='data:image/png;base64,' class='_logo'></image> 下载这首歌
  </button>
  <!-- 背景色 -->
  <view class='page_bg' style='background:url("{{obj.cover_url}}") no-repeat;'></view>
  <!--遮罩  -->
  <view class='bg_mask' style='opacity:0.76'></view>
</view>
