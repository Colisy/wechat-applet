<view class='page'>
  <view class='page_content'>
    <!--loadmore  -->
    <scroll-view style="height:100vh" scroll-y="true" bindscrolltolower="lower">
      <!-- lists-->
      <view class='lists-Outerv' wx:if="{{isLists}}">
        <view class='list'>
          <view class="itemTitle">
            <image src='data:image/png;base64,'></image>
            <label>播放全部</label>
          </view>
        </view>
        <view wx:for="{{lists}}" class='list listItems' wx:key='*this' id='{{item.id}}'>
          <view class='list-children' bindtouchstart="touchStart" bindtouchmove="touchMove" bindtouchend="touchEnd" style="{{item.txtStyle}}" data-id="{{item.id}}" data-index="{{index}}" bindtap='gotoPlayControlle'>
            <view class="itemTitle">{{item.title}}</view>
            <view class="itemTitle _itemDes">{{item.nickname}}- {{item.title}}</view>
          </view>
          <view class='list-t-c' data-id='{{item.id}}' bindtap='delectItem'>刪除</view>
        </view>
      </view>
      <!--没有结果  -->
      <view class='noReault' wx:if='{{!isLists}}'>
        <image src='data:image/png;base64,'></image>
        <view class='no_result'>无匹配</view>
        <view>很抱歉，{{noResult}}</view>
      </view>
    </scroll-view>
  </view>

  <!--背景  -->
  <view class='page__bg'>
  </view>
</view>
