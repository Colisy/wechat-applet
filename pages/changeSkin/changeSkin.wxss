/* pages/changeSkin/changeSkin.wxss */
@import '../../style/person.wxss';
/*header  */
.page__hd{
  text-align: left;
  color:#2f2f31;
  font-size:0.9rem;
  padding:1rem;
}
/*左侧  */
.page__span{
  width:0.2rem;
  height: 1rem;
  position:absolute;
  left:0;
  display:inline-block;
  background-color: #ff5045;
}
/*皮肤列表  */
.skin_list{
  height:11rem;
  display: flex;
  justify-content: space-between;
}
.skin-item{  
   width:32%; 
   height:100%;
   font-size: 0.8rem;
   color:#303032;
}
/*官方红  */
.skin-item-red,.skin-item-white,.skin-item-user{
  height:8rem;
  position: relative;
  border:1px solid #cbcbcb;
  background-image: url('data:image/jpeg;base64,');
  background-size: 100% 100%;
  background-position: center center;
  background-repeat: no-repeat;
  margin-bottom:0.4rem;
}
.skin-item-white{
  background-image: url('data:image/jpeg;base64,');
}
.useing{
  position: absolute;
  width:1.6rem;
  height: 1.6rem;
  right:0.2rem;
  bottom:0.2rem;
}
/*文字描述  */
.skin-item text{
  /* font-size: 0.6rem; */
  color:#808183;
}
/*官方白  */
.skin-item-hotRed{
  background-color: #d53e35;
}
/*自定义  */
.skin-item-user{
  background:linear-gradient(to bottom,#d33a32,#e3e3e3,#a0cbff,#6b7cfe,#0169fe,#be00fe);
}









