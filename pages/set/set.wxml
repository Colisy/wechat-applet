<!--pages/set/set.wxml-->
<view class='page'>
  <view class='page__cd'>
    <view class='list'>
      在线播放音质
      <text class='rightAnsMore'>自动</text>
      <image class='_more' src='../../images/right.png'></image>
    </view>
    <view class='list border_botom'>
      下载音质
      <text class='rightAnsMore'>标准</text>
      <image class='_more' src='../../images/right.png'></image>
    </view>
    <view class='list' bindtap='networkplay'>
      使用2G/3G/4G网络播放
      <image class='_more _more3' src='data:image/png;base64,' wx:if="{{!isPlay}}"></image>
      <image class='_more _more3' src='data:image/png;base64,' wx:if="{{isPlay}}"></image>
    </view>
    <view class='list border_botom' bindtap='networkdownload'>
      使用2G/3G/4G网络下载
      <image class='_more _more3' src='data:image/png;base64,' wx:if="{{!isDown}}"></image>
      <image class='_more _more3' src='data:image/png;base64,' wx:if="{{isDown}}"></image>
    </view>
  </view>

  <!--背景  -->
  <view class='page__bg'></view>
</view>
