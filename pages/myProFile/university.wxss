/* pages/myProFile/username.wxss */
@import '../../style/person.wxss';
/*背景  */
.page__bg{
  background-color: #fbfcfe;
}
/*列表  */
.list{
  background: #eeeff1;
  border:0;
}
/*清除  */
.weui-icon-clear{
  top:0.5rem;
  right:0.9rem;
  z-index:4;
}
/*icon  */
.page__cd .list>image{
  width:1rem;
  height: 1rem;
  float: left;
  margin-top: 0.2rem;
}
/*input  */
.page__cd .list input{
  width:90%;
  border-radius:30px;
  padding:0 1rem;
  background-color: #fbfcfe;
  position:relative;
  z-index: 3;
}





