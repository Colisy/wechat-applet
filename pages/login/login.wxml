<!--pages/login/login.wxml-->
<view class='page'>
  <view class='page_content'>
    <!--input  -->
    <view class='inputBox'>
      <view>
        <image src='data:image/png;base64,'></image>
        <input value='{{username}}' auto-focus='true' focus='true' bindinput='username' placeholder-class='input_placeholder' number type='text' placeholder='手机号' />
        <view class="weui-icon-clear" wx:if="{{username.length > 0}}" bindtap="clearInput">
          <icon type="clear" size="14"></icon>
        </view>
      </view>
      <view>
        <image src='data:image/png;base64,'></image>
        <input value='{{password}}' bindinput='password' placeholder-class='input_placeholder' type='text' password placeholder='密码' />
        <view class="weui-icon-clear" wx:if="{{password.length > 0}}" bindtap="clearpassword">
          <icon type="clear" size="14"></icon>
        </view>
      </view>
    </view>
    <!--登录按钮  -->
    <button class="weui-btn submit" bindtap='loginCommit'>登录</button>
    <!--重设密码  -->
    <view class='aboutPassword'>
      <text  bindtap='forgetPassword'>重设密码</text>
    </view>
  </view>
  <!--背景  -->
  <view class='page_pg'></view>
</view>