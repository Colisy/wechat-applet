<view class='page'>
  <!--tabbar -->
  <view class="NavBarpage">
    <view class="weui-tab">
      <view class="weui-navbar">
        <block wx:for="{{tabs}}" wx:key="*this">
          <view id="{{index}}" class="weui-navbar__item {{activeIndex == index ? 'weui-bar__item_on' : ''}}" bindtap="tabClick">
            <view class="weui-navbar__title">{{item}}</view>
          </view>
        </block>
        <view class="weui-navbar__slider" style="left: {{sliderLeft}}px; transform: translateX({{sliderOffset}}px); -webkit-transform: translateX({{sliderOffset}}px);"></view>
      </view>
      <view class="weui-tab__panel">
        <!--option a -->
        <view class="weui-tab__content" hidden="{{activeIndex != 0}}">
          <!-- swiper -->
          <swiper indicator-dots="{{indicatorDots}}" indicator-color="{{indicatorColor}}" indicator-active-color="{{indicatorAactiveColor}}" autoplay='{{autoplay}}' interval='{{interval}}'>
            <block wx:for="{{imgUrls}}" wx:key='*this'>
              <swiper-item class='swiper_item'>
                <image src="{{item}}" class="slide-image" />
              </swiper-item>
            </block>
          </swiper>
          <!--loadmore  -->
          <scroll-view style="height:30rem" scroll-y="true" bindscrolltolower="lower">
            <!-- songs lists-->
            <view class='lists-Outer'>
              <view wx:for="{{lists}}" id='{{item.id}}' class='list home_listItems listItems' wx:key='*this' bindtap='gotoPlayControlle'>
                <view class="itemIndex {{index==0 ? 'gold' : index==1 ? 'silver' : index==2 ? 'copper' : ''}}">{{index>2 ? index+1 : ''}}</view>
                <view class="itemTitle">{{item.title}}
                  <image src='data:image/png;base64,' wx:if="{{item.isvip==1?true:false}}"></image>
                </view>
                <view class="itemTitle _itemDes">{{item.nickname}}- {{item.title}}</view>
              </view>
            </view>
          </scroll-view>
        </view>
        <!--option b  -->
        <view class="weui-tab__content" hidden="{{activeIndex != 1}}">
          <!-- swiper -->
          <swiper indicator-dots="{{indicatorDots}}" indicator-color="{{indicatorColor}}" indicator-active-color="{{indicatorAactiveColor}}" autoplay='{{autoplay}}' interval='{{interval}}'>
            <block wx:for="{{imgUrls}}" wx:key='*this'>
              <swiper-item class='swiper_item'>
                <image src="{{item}}" class="slide-image" />
              </swiper-item>
            </block>
          </swiper>
          <!--vip-->
          <!--loadmore  -->
          <scroll-view style="height:30rem" scroll-y="true" bindscrolltolower="viplower">
            <view class='lists-Outer'>
              <view wx:for="{{vipLists}}" id='{{item.id}}' class='list _listItems listItems' wx:key='*this' bindtap='gotoPlayControlle'>
                <view class="itemTitle">{{item.title}}
                  <image src='data:image/png;base64,'></image>
                </view>
                <view class="itemTitle _itemDes">{{item.nickname}}- {{item.title}}</view>
              </view>
            </view>
          </scroll-view>
          <!--  -->
        </view>
      </view>
    </view>
  </view>
</view>
